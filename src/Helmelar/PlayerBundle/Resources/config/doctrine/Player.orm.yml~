Helmelar\SiteBundle\Entity\Player:
    type: entity
    table: PLAYER
    fields:
        playerId:
            id: true
            type: integer
            unsigned: false
            nullable: false
            column: PLAYER_ID
            generator:
                strategy: IDENTITY
        userId:
            type: integer
            unsigned: false
            nullable: false
            column: USER_ID
        playerBirthdate:
            type: date
            nullable: false
            column: PLAYER_BIRTHDATE
        playerCreationDate:
            type: datetime
            nullable: false
            column: PLAYER_CREATION_DATE
        playerHin:
            type: string
            length: 12
            fixed: false
            nullable: false
            column: PLAYER_HIN
        playerSickness:
            type: text
            nullable: false
            column: PLAYER_SICKNESS
    lifecycleCallbacks: {  }
