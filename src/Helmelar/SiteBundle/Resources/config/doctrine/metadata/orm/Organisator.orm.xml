<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Organisator" table="ORGANISATOR">
    <id name="organisatorId" type="integer" column="ORGANISATOR_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="USER_ID" nullable="false"/>
    <field name="organisatorRefId" type="integer" column="ORGANISATOR_REF_ID" nullable="false"/>
    <field name="organisatorTitle" type="string" column="ORGANISATOR_TITLE" length="55" nullable="false"/>
    <field name="organisatorDate" type="datetime" column="ORGANISATOR_DATE" nullable="false"/>
  </entity>
</doctrine-mapping>
