<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Player" table="PLAYER">
    <id name="playerId" type="integer" column="PLAYER_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="USER_ID" nullable="false"/>
    <field name="playerBirthdate" type="date" column="PLAYER_BIRTHDATE" nullable="false"/>
    <field name="playerCreationDate" type="datetime" column="PLAYER_CREATION_DATE" nullable="false"/>
    <field name="playerHin" type="string" column="PLAYER_HIN" length="12" nullable="false"/>
    <field name="playerSickness" type="text" column="PLAYER_SICKNESS" nullable="false"/>
  </entity>
</doctrine-mapping>
