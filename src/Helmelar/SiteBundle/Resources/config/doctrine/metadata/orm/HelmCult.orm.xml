<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HelmCult" table="HELM_CULT">
    <id name="helmCultId" type="integer" column="HELM_CULT_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="helmCultName" type="string" column="HELM_CULT_NAME" length="55" nullable="false"/>
    <field name="helmCultDivinPlan" type="boolean" column="HELM_CULT_DIVIN_PLAN" nullable="false"/>
    <field name="helmCultDescription" type="text" column="HELM_CULT_DESCRIPTION" nullable="true"/>
    <field name="helmCultSymbol" type="text" column="HELM_CULT_SYMBOL" nullable="false"/>
    <field name="helmCultPhilosophy" type="text" column="HELM_CULT_PHILOSOPHY" nullable="false"/>
  </entity>
</doctrine-mapping>
