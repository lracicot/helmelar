<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Actor" table="ACTOR">
    <id name="actorId" type="integer" column="ACTOR_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="USER_ID" nullable="false"/>
    <field name="organisatorRefId" type="integer" column="ORGANISATOR_REF_ID" nullable="false"/>
    <field name="actorTitle" type="string" column="ACTOR_TITLE" length="55" nullable="false"/>
    <field name="actorDate" type="datetime" column="ACTOR_DATE" nullable="false"/>
  </entity>
</doctrine-mapping>
