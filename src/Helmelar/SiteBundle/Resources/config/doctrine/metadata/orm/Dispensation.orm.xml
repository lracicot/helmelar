<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Dispensation" table="DISPENSATION">
    <id name="dispensationId" type="integer" column="DISPENSATION_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="playerId" type="integer" column="PLAYER_ID" nullable="false"/>
    <field name="dispensationPresentation" type="text" column="DISPENSATION_PRESENTATION" nullable="false"/>
    <field name="dispensationBg" type="text" column="DISPENSATION_BG" nullable="false"/>
    <field name="dispensationImage" type="string" column="DISPENSATION_IMAGE" length="255" nullable="false"/>
    <field name="dispensationDate" type="datetime" column="DISPENSATION_DATE" nullable="false"/>
    <field name="dispensationAprovalDate" type="datetime" column="DISPENSATION_APROVAL_DATE" nullable="false"/>
    <field name="organisatorId" type="integer" column="ORGANISATOR_ID" nullable="false"/>
  </entity>
</doctrine-mapping>
