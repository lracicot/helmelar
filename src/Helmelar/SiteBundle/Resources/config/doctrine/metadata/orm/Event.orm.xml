<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Event" table="EVENT">
    <id name="eventId" type="integer" column="EVENT_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventName" type="string" column="EVENT_NAME" length="255" nullable="false"/>
    <field name="eventDateStart" type="datetime" column="EVENT_DATE_START" nullable="false"/>
    <field name="eventDateEnd" type="datetime" column="EVENT_DATE_END" nullable="false"/>
    <field name="eventTime" type="time" column="EVENT_TIME" nullable="false"/>
    <field name="eventDescription" type="text" column="EVENT_DESCRIPTION" nullable="false"/>
    <field name="eventType" type="boolean" column="EVENT_TYPE" nullable="false"/>
    <field name="eventPrice" type="float" column="EVENT_PRICE" nullable="false"/>
    <field name="eventReducePrice" type="float" column="EVENT_REDUCE_PRICE" nullable="false"/>
    <field name="eventPreinscript" type="boolean" column="EVENT_PREINSCRIPT" nullable="false"/>
    <field name="eventPreBrefing" type="text" column="EVENT_PRE_BREFING" nullable="false"/>
  </entity>
</doctrine-mapping>
