<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HelmSpell" table="HELM_SPELL">
    <id name="helmSpellId" type="integer" column="HELM_SPELL_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="helmSpellType" type="boolean" column="HELM_SPELL_TYPE" nullable="false"/>
    <field name="helmSpellLvl" type="boolean" column="HELM_SPELL_LVL" nullable="false"/>
    <field name="helmSpellPlan" type="boolean" column="HELM_SPELL_PLAN" nullable="false"/>
    <field name="helmSpellGodId" type="integer" column="HELM_SPELL_GOD_ID" nullable="false"/>
    <field name="helmSpellRaceId" type="integer" column="HELM_SPELL_RACE_ID" nullable="false"/>
    <field name="helmSpellName" type="string" column="HELM_SPELL_NAME" length="255" nullable="false"/>
    <field name="helmSpellRuneId" type="integer" column="HELM_SPELL_RUNE_ID" nullable="false"/>
    <field name="helmSpellCast" type="text" column="HELM_SPELL_CAST" nullable="false"/>
    <field name="helmSpellFinalWords" type="text" column="HELM_SPELL_FINAL_WORDS" nullable="false"/>
    <field name="helmSpellEffect" type="text" column="HELM_SPELL_EFFECT" nullable="false"/>
    <field name="helmSpellDuration" type="string" column="HELM_SPELL_DURATION" length="255" nullable="false"/>
    <field name="helmSpellSpecialDuration" type="string" column="HELM_SPELL_SPECIAL_DURATION" length="70" nullable="false"/>
    <field name="helmSpellReagent" type="text" column="HELM_SPELL_REAGENT" nullable="false"/>
    <field name="helmSpellRestriction" type="text" column="HELM_SPELL_RESTRICTION" nullable="false"/>
    <field name="helmSpellSpecial" type="text" column="HELM_SPELL_SPECIAL" nullable="false"/>
    <field name="helmSpellHistory" type="text" column="HELM_SPELL_HISTORY" nullable="false"/>
    <field name="helmSpellCreatorId" type="integer" column="HELM_SPELL_CREATOR_ID" nullable="false"/>
    <field name="helmSpellDate" type="datetime" column="HELM_SPELL_DATE" nullable="false"/>
    <field name="helmSpellDateRev" type="datetime" column="HELM_SPELL_DATE_REV" nullable="false"/>
    <field name="helmSpellNote" type="text" column="HELM_SPELL_NOTE" nullable="false"/>
  </entity>
</doctrine-mapping>
