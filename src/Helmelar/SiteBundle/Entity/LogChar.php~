<?php

namespace Helmelar\SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Helmelar\SiteBundle\Entity\LogChar
 *
 * @ORM\Table(name="LOG_CHAR")
 * @ORM\Entity
 */
class LogChar
{
    /**
     * @var integer $logCharId
     *
     * @ORM\Column(name="LOG_CHAR_ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $logCharId;

    /**
     * @var \DateTime $logCharDatetime
     *
     * @ORM\Column(name="LOG_CHAR_DATETIME", type="datetime", nullable=false)
     */
    private $logCharDatetime;

    /**
     * @var integer $userId
     *
     * @ORM\Column(name="USER_ID", type="integer", nullable=false)
     */
    private $userId;

    /**
     * @var integer $charId
     *
     * @ORM\Column(name="CHAR_ID", type="integer", nullable=false)
     */
    private $charId;

    /**
     * @var string $logCharField
     *
     * @ORM\Column(name="LOG_CHAR_FIELD", type="string", length=255, nullable=false)
     */
    private $logCharField;

    /**
     * @var string $logCharOldValue
     *
     * @ORM\Column(name="LOG_CHAR_OLD_VALUE", type="string", length=255, nullable=false)
     */
    private $logCharOldValue;

    /**
     * @var string $logCharNewValue
     *
     * @ORM\Column(name="LOG_CHAR_NEW_VALUE", type="string", length=255, nullable=false)
     */
    private $logCharNewValue;


}
