<?php

namespace Helmelar\SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Helmelar\SiteBundle\Entity\LogEmailReason
 *
 * @ORM\Table(name="LOG_EMAIL_REASON")
 * @ORM\Entity
 */
class LogEmailReason
{
    /**
     * @var integer $logEmailReasonId
     *
     * @ORM\Column(name="LOG_EMAIL_REASON_ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $logEmailReasonId;

    /**
     * @var string $logEmailReasonTitle
     *
     * @ORM\Column(name="LOG_EMAIL_REASON_TITLE", type="string", length=255, nullable=false)
     */
    private $logEmailReasonTitle;

    /**
     * @var string $logEmailReason
     *
     * @ORM\Column(name="LOG_EMAIL_REASON", type="text", nullable=false)
     */
    private $logEmailReason;


}
