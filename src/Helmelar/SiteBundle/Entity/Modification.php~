<?php

namespace Helmelar\SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Helmelar\SiteBundle\Entity\Modification
 *
 * @ORM\Table(name="MODIFICATION")
 * @ORM\Entity
 */
class Modification
{
    /**
     * @var integer $modificationId
     *
     * @ORM\Column(name="MODIFICATION_ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $modificationId;

    /**
     * @var integer $playerId
     *
     * @ORM\Column(name="PLAYER_ID", type="integer", nullable=false)
     */
    private $playerId;

    /**
     * @var integer $charactId
     *
     * @ORM\Column(name="CHARACT_ID", type="integer", nullable=false)
     */
    private $charactId;

    /**
     * @var string $modificationField
     *
     * @ORM\Column(name="MODIFICATION_FIELD", type="string", length=255, nullable=false)
     */
    private $modificationField;

    /**
     * @var string $modificationChangeFrom
     *
     * @ORM\Column(name="MODIFICATION_CHANGE_FROM", type="text", nullable=false)
     */
    private $modificationChangeFrom;

    /**
     * @var string $modificationTo
     *
     * @ORM\Column(name="MODIFICATION_TO", type="text", nullable=false)
     */
    private $modificationTo;

    /**
     * @var \DateTime $modificationDate
     *
     * @ORM\Column(name="MODIFICATION_DATE", type="datetime", nullable=false)
     */
    private $modificationDate;

    /**
     * @var integer $organisatorId
     *
     * @ORM\Column(name="ORGANISATOR_ID", type="integer", nullable=false)
     */
    private $organisatorId;

    /**
     * @var boolean $modificationStatus
     *
     * @ORM\Column(name="MODIFICATION_STATUS", type="boolean", nullable=false)
     */
    private $modificationStatus;


}
