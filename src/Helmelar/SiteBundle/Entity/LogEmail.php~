<?php

namespace Helmelar\SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Helmelar\SiteBundle\Entity\LogEmail
 *
 * @ORM\Table(name="LOG_EMAIL")
 * @ORM\Entity
 */
class LogEmail
{
    /**
     * @var integer $logEmailId
     *
     * @ORM\Column(name="LOG_EMAIL_ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $logEmailId;

    /**
     * @var integer $logEmailUserId
     *
     * @ORM\Column(name="LOG_EMAIL_USER_ID", type="integer", nullable=false)
     */
    private $logEmailUserId;

    /**
     * @var integer $logEmailSenderUserId
     *
     * @ORM\Column(name="LOG_EMAIL_SENDER_USER_ID", type="integer", nullable=false)
     */
    private $logEmailSenderUserId;

    /**
     * @var \DateTime $logEmailDateSend
     *
     * @ORM\Column(name="LOG_EMAIL_DATE_SEND", type="datetime", nullable=false)
     */
    private $logEmailDateSend;

    /**
     * @var \DateTime $logEmailDateStamp
     *
     * @ORM\Column(name="LOG_EMAIL_DATE_STAMP", type="datetime", nullable=false)
     */
    private $logEmailDateStamp;

    /**
     * @var \DateTime $logEmailDateView
     *
     * @ORM\Column(name="LOG_EMAIL_DATE_VIEW", type="datetime", nullable=false)
     */
    private $logEmailDateView;

    /**
     * @var boolean $logEmailReason
     *
     * @ORM\Column(name="LOG_EMAIL_REASON", type="boolean", nullable=false)
     */
    private $logEmailReason;


}
