    type: entity
    table: SCENARIO
    fields:
        scenarioId:
            id: true
            type: integer
            unsigned: false
            nullable: false
            column: SCENARIO_ID
            generator:
                strategy: IDENTITY
        eventId:
            type: integer
            unsigned: false
            nullable: false
            column: EVENT_ID
        organisatorId:
            type: integer
            unsigned: false
            nullable: false
            column: ORGANISATOR_ID
        scenarioDate:
            type: datetime
            nullable: false
            column: SCENARIO_DATE
    lifecycleCallbacks: {  }
