HelmAlign:
    type: entity
    table: HELM_ALIGN
    fields:
        helmAlignId:
            id: true
            type: integer
            unsigned: false
            nullable: false
            column: HELM_ALIGN_ID
            generator:
                strategy: IDENTITY
        helmAlignName:
            type: string
            length: 20
            fixed: false
            nullable: false
            column: HELM_ALIGN_NAME
        helmBehaviorId:
            type: boolean
            nullable: false
            column: HELM_BEHAVIOR_ID
        helmAlignDescription:
            type: text
            nullable: true
            column: HELM_ALIGN_DESCRIPTION
        helmAlignOrder:
            type: boolean
            nullable: false
            column: HELM_ALIGN_ORDER
    lifecycleCallbacks: {  }
