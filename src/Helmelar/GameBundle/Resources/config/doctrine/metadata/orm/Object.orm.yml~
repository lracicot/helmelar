HelmObject:
    type: entity
    table: HELM_OBJECT
    fields:
        helmObjectId:
            id: true
            type: integer
            unsigned: false
            nullable: false
            column: HELM_OBJECT_ID
            generator:
                strategy: IDENTITY
        helmObjectName:
            type: string
            length: 200
            fixed: false
            nullable: false
            column: HELM_OBJECT_NAME
        helmObjectNumber:
            type: integer
            unsigned: false
            nullable: false
            column: HELM_OBJECT_NUMBER
        helmObjectStatus:
            type: boolean
            nullable: false
            column: HELM_OBJECT_STATUS
        helmObjectType:
            type: boolean
            nullable: true
            column: HELM_OBJECT_TYPE
        helmObjectCatId:
            type: integer
            unsigned: false
            nullable: false
            column: HELM_OBJECT_CAT_ID
        helmObjectSubcatId:
            type: integer
            unsigned: false
            nullable: false
            column: HELM_OBJECT_SUBCAT_ID
        helmObjectIdLvl:
            type: boolean
            nullable: false
            column: HELM_OBJECT_ID_LVL
        helmObjectControlLvl:
            type: boolean
            nullable: false
            column: HELM_OBJECT_CONTROL_LVL
        helmObjectDescription:
            type: text
            nullable: true
            column: HELM_OBJECT_DESCRIPTION
        helmObjectDescriptionEffectControl:
            type: text
            nullable: false
            column: HELM_OBJECT_DESCRIPTION_EFFECT_CONTROL
        helmObjectDescriptionEffectNoControl:
            type: text
            nullable: false
            column: HELM_OBJECT_DESCRIPTION_EFFECT_NO_CONTROL
        helmObjectHistory:
            type: text
            nullable: false
            column: HELM_OBJECT_HISTORY
        helmObjectSpecial:
            type: text
            nullable: false
            column: HELM_OBJECT_SPECIAL
        charactId:
            type: integer
            unsigned: false
            nullable: false
            column: CHARACT_ID
        helmObjectNote:
            type: text
            nullable: false
            column: HELM_OBJECT_NOTE
    lifecycleCallbacks: {  }
