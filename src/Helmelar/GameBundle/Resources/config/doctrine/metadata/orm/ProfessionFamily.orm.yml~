    type: entity
    table: HELM_PROFESSION_FAMILY
    fields:
        helmProfessionFamilyId:
            id: true
            type: integer
            unsigned: false
            nullable: false
            column: HELM_PROFESSION_FAMILY_ID
            generator:
                strategy: IDENTITY
        helmProfessionFamilyName:
            type: string
            length: 20
            fixed: false
            nullable: false
            column: HELM_PROFESSION_FAMILY_NAME
        helmProfessionFamilyDescription:
            type: text
            nullable: true
            column: HELM_PROFESSION_FAMILY_DESCRIPTION
    lifecycleCallbacks: {  }
